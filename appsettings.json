{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "StatusBarOption": {
    "Interval": 1,
    "Display": [
      {
        "Target": "Processor",
        "Format": "\udb81\ude28 total_usage"
      },
      {
        "Target": "Network",
        "Select": "enp34s0",
        "Format": "\udb83\udc53 \uDB81\uDD52 up_speed, \uDB80\uDDDA down_speed  \uDB81\uDCC5 bandwidth",
	"Color": "#F1FA8C"
      },
      {
        "Target": "Datetime",
        "Format": "\uDB82\uDD54 yyyy-MM-dd HH:mm:ss",
	"Color": "#F1FA8C"
      }
    ]
  }
}
